	note = {Publisher: Cell Press},
	file = {PDF:/home/agarpe/Zotero/storage/QSW8EPD2/full-text.pdf:application/pdf},
}

@article{stecklow_alteracoes_2010,
	title = {Alterações no {EEG} durante sequencias de imagética motora visual e cinestésica},
