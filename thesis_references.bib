	langid = {english},
	note = {Publisher: Nature Publishing Group},
	keywords = {Network models, Computer science, Electrical and electronic engineering},
	file = {Submitted Version:/home/agarpe/Zotero/storage/XYVXSZBP/Woźniak et al. - 2020 - Deep learning incorporating biologically inspired .pdf:application/pdf},
